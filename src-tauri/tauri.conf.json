{
  "productName": "deps",
  "version": "0.1.0",
  "identifier": "com.deps.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "appimage", "rpm"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {},
  "app": {
    "windows": [
      {
        "title": "Deps",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Main app capabilities",
          "windows": ["main"],
          "permissions": [
            "core:default",
            "dialog:allow-open",
            "store:allow-get",
            "store:allow-set",
            "store:allow-save",
            "store:default",
            "shell:default"
          ]
        }
      ]
    }
  }
}
